InjectSkillInfo(SKILLID_QuickRiposte, SkillInfo_QuickRiposte)



// Icons
ALIGN 4
SI_Vantage:
	#incbin "Icons/Vantage.4bpp"

SI_VantageBattalion:
	#incbin "Icons/VantageBattalion.4bpp"

