ALIGN 4
SkillInfo_Vantage:
	/* desc */ SHORT msg_SD_Vantage
	/* name */ SHORT msg_SN_Vantage
	/* icon */ POIN SI_Vantage
	
SkillInfo_VantageBattalion:
	/* desc */ SHORT msg_SD_VantageBattalion
	/* name */ SHORT msg_SN_VantageBattalion
	/* icon */ POIN SI_VantageBattalion
	
SkillInfo_Desperation:
	/* desc */ SHORT msg_SD_Desperation
	/* name */ SHORT msg_SN_Desperation
	/* icon */ POIN SI_Desperation
	
SkillInfo_DesperationBattalion:
	/* desc */ SHORT msg_SD_DesperationBattalion
	/* name */ SHORT msg_SN_DesperationBattalion
	/* icon */ POIN SI_DesperationBattalion
	
SkillInfo_QuickRiposte:
	/* desc */ SHORT msg_SD_QuickRiposte
	/* name */ SHORT msg_SN_QuickRiposte
	/* icon */ POIN SI_QuickRiposte


InjectSkillInfo(SKILLID_Vantage, SkillInfo_Vantage)
InjectSkillInfo(SKILLID_VantageBattalion, SkillInfo_VantageBattalion)
InjectSkillInfo(SKILLID_Desperation, SkillInfo_Desperation)
